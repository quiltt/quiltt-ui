{
  "name": "@quiltt/ui",
  "version": "1.0.1",
  "description": "Open source fintech react components designed to work with the Quiltt API",
  "author": "Zubair Aziz",
  "license": "MIT",
  "repository": "quiltt/quiltt-ui",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "echo \"Write tests\"",
    "build": "rollup -c",
    "develop": "rollup -c -w",
    "start": "yarn develop",
    "precommit": "lint-staged",
    "prepare": "npm run build",
    "package": "npm np",
    "publish-script": "npm publish --access public",
    "predeploy": "cd example && yarn install && yarn build",
    "deploy": "gh-pages -d example/dist",
    "lint": "eslint --ext .tsx --ext .ts src/ && eslint --ext .tsx --ext .ts example/src/ ",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix && eslint --ext .tsx --ext .ts example/src/ --fix",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "type-check": "tsc"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@svgr/rollup": "^5.5.0",
    "@tailwindcss/forms": "^0.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "autoprefixer": "^10.3.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "np": "^7.5.0",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.3.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "^4.0.3",
    "rollup": "^2.53.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-url": "^3.0.1",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.1",
    "react-focus-lock": "^2.5.2",
    "react-plaid-link": "^3.2.0"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "yarn lint:fix",
      "yarn format"
    ]
  }
}
