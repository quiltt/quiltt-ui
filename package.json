{
  "name": "@quiltt/ui",
  "version": "1.1.10",
  "description": "Open source react components designed to work with the Quiltt API",
  "author": "Zubair Aziz",
  "license": "MIT",
  "repository": "quiltt/quiltt-ui",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/src/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --collect-coverage",
    "clean": "rimraf dist",
    "build": "rollup -c",
    "build:prod": "yarn clean && rollup -c",
    "prepare": "yarn build:prod",
    "develop": "rollup -c -w",
    "start": "yarn develop",
    "publish-script": "npm publish --access public",
    "lint": "eslint --ext .tsx --ext .ts src/",
    "lint:fix": "eslint --ext .tsx --ext .ts src/ --fix",
    "prettier": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,md,json}\"",
    "format": "yarn lint:fix && yarn prettier",
    "type-check": "tsc"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
