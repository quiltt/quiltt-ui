    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-url": "^3.0.1",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^27.1.4",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.8",
