    "@heroicons/react": "^1.0.5",
    "@hookform/error-message": "^2.0.0",
    "@hookform/resolvers": "^2.8.5",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "react-focus-lock": "^2.7.1",
